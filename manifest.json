{	
	"name": "spot-notes",
	"version": "1.1",
	"manifest_version": 2,
	"browser_action":{
		"default_popup": "login_default.html", 
		"default_title": "Spot Notes!",
		"default_icon": {
			"16": "icons/icon_16.png",
			"48": "icons/icon_48.png",
			"128": "icons/icon_128.png"
		}
	},
	"background": {
		"scripts": [ "background.js", 
					 "scheduler.js"],
		"persistent": false
	},
	"permissions": [
		"storage",
		"identity",
		"alarms",
		"tabs",
		"notifications",
		"https://*/*"
	]
}