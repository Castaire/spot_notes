{	
	"name": "spot-notes",
	"version": "0.1",
	"manifest_version": 2,
	"browser_action":{
		"default_popup": "login_default.html", 
		"default_title": "Spot Notes!"
	},
	"background": {
		"scripts": [ "background.js", 
					 "scheduler.js"],
		"persistent": false
	},
	"permissions": [
		"storage",
		"identity",
		"alarms",
		"activeTab",
		"tabs",
		"https://*/*"
	]
}